{
  "name": "training-backend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "10.13.0"
  },
  "scripts": {
    "build": "babel src -d dist --copy-files",
    "lint": "eslint --fix ./src",
    "dev": "nodemon --exec babel-node src/server.js",
    "start": "env TZ='Etc/UTC' node ./dist/server.js",
    "migrate:init": "cd database && node createDB.js && cd .. && npm run migrate",
    "migrate:undo": "cd database && sequelize db:migrate:undo:all --config '../config/local.json' --env database",
    "migrate": "cd database && sequelize db:migrate --config '../config/local.json' --env database && sequelize db:seed:all --config '../config/local.json' --env database"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "@hapi/joi-date": "^1.3.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto-js": "^4.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "format": "^0.2.2",
    "helmet": "^4.1.0",
    "i18n": "^0.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "mysql2": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "query-string": "^6.13.1",
    "rimraf": "^2.7.1",
    "rotating-file-stream": "^1.4.6",
    "sequelize": "^5.22.3",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
